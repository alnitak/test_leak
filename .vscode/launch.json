    "version": "0.2.0",
    "configurations": [
        {
            "name": "test_leak",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "test_leak (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "test_leak (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "local engine",
            "request": "launch",
            "type": "dart",
            "args": [
                    "--local-engine", "host_debug_unopt", 
                    "--local-engine-host", "host_debug_unopt",
                    "--local-engine-src-path", "~/FLUTTER/flutter/engine/src",
                    "--trace-skia"
                ]
          }
    ]
}